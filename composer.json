{
	"name": "wapplersystems/messenger",
	"type": "typo3-cms-extension",
	"description": "Default symfony messenger commands",
	"keywords": [
		"TYPO3",
		"extension"
	],
	"homepage": "https://wappler.systems",
	"authors": [
		{
			"name": "Sven Wappler",
			"role": "Developer",
			"homepage": "https://wappler.systems"
		}
	],
	"license": ["GPL-2.0-or-later"],
	"require": {
		"typo3/cms-core": "^10.4 || ^11.0",
		"typo3/cms-extbase": "^10.4 || ^11.0",
		"symfony/messenger": "^5.4 || ^6.0"
	},
	"suggest": {
	},
	"require-dev": {
		"symplify/easy-coding-standard": "^11.1"
	},
	"autoload": {
		"psr-4": {
			"WapplerSystems\\Messenger\\": "Classes"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "messenger"
		}
	},
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": false,
			"typo3/cms-composer-installers": false
		}
	},
	"scripts": {
		"check-style": "ecs check --ansi",
		"fix-style": [
			"ecs check --fix --ansi"
		]
	}
}
